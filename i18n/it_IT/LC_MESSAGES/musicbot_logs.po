#, python-format
msgid "Pausing MusicPlayer due to network availability:  (%(guild_id)s) %(player)r"
msgstr "Pausa MusicPlayer a causa della disponibilità della rete: (%(guild_id)s) %(player)r"

#: musicbot/bot.py:555
#, python-format
msgid "Looking for owner in guild: %(guild)s (required voice: %(required)s) and got:  %(owner)s"
msgstr "Alla ricerca del proprietario nella gilda: %(guild)s (voce obbligatoria: %(required)s) e ottiene:  %(owner)s"

